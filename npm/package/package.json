{
  "name": "@vishyfishy2/terminalfont-detect",
  "version": "0.1.5",
  "description": "Cross-platform terminal font detector using fastfetch",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js"
    }
  },
  "bin": {
    "terminalfont-detect": "./dist/cli.js",
    "@vishyfishy2/terminalfont-detect": "./dist/cli.js"
  },
  "scripts": {
    "build": "zshy",
    "prepublishOnly": "bun run build"
  },
  "zshy": {
    "outDir": "./dist",
    "exports": {
      ".": "./src/index.ts",
      "./cli": "./src/cli.ts"
    }
  },
  "optionalDependencies": {
    "@vishyfishy2/terminalfont-detect-darwin-arm64": "^0.1.4",
    "@vishyfishy2/terminalfont-detect-darwin-x64": "^0.1.4",
    "@vishyfishy2/terminalfont-detect-linux-x64-gnu": "^0.1.4",
    "@vishyfishy2/terminalfont-detect-linux-x64-musl": "^0.1.4",
    "@vishyfishy2/terminalfont-detect-linux-arm64-gnu": "^0.1.4",
    "@vishyfishy2/terminalfont-detect-linux-arm64-musl": "^0.1.4",
    "@vishyfishy2/terminalfont-detect-win32-x64": "^0.1.4"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "keywords": [
    "terminal",
    "font",
    "detect",
    "fastfetch"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2",
    "zshy": "^0.4.2"
  }
}
